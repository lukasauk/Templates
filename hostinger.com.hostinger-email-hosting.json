{
    "providerId": "hostinger.com",
    "providerName": "Hostinger",
    "serviceId": "hostinger-email-hosting",
    "serviceName": "Hostinger Email Hosting",
    "logoUrl": "https://www.domainconnect.org/wp-content/uploads/2022/09/logo-horizontal.png",
    "description": "Setup all necessary DNS records to enable Hostinger Email",
    "variableDescription": "",
    "version": 1,
    "records": [
        {
            "groupId": "mx-setup",
            "type": "MX",
            "host": "@",
            "priority": 5,
            "pointsTo": "mx1.hostinger.com",
            "ttl": 3600
        },
        {
            "groupId": "mx-setup",
            "type": "MX",
            "host": "@",
            "priority": 10,
            "pointsTo": "mx2.hostinger.com",
            "ttl": 3600
        },
        {
            "groupId": "spf-setup",
            "type": "SPFM",
            "host": "@",
            "spfRules": "include:_spf.mail.hostinger.com"
        },
        {
            "groupId": "dkim-setup",
            "pointsTo": "hostingermail-a.dkim.mail.hostinger.com",
            "type": "CNAME",
            "host": "hostingermail-a._domainkey",
            "ttl": 3600
        },
        {
            "groupId": "dkim-setup",
            "pointsTo": "hostingermail-b.dkim.mail.hostinger.com",
            "type": "CNAME",
            "host": "hostingermail-b._domainkey",
            "ttl": 3600
        },
        {
            "groupId": "autodiscover-setup",
            "pointsTo": "autodiscover.mail.hostinger.com",
            "type": "CNAME",
            "host": "autodiscover",
            "ttl": 3600
        },
        {
            "groupId": "autoconfig-setup",
            "pointsTo": "autoconfig.mail.hostinger.com",
            "type": "CNAME",
            "host": "autoconfig",
            "ttl": 3600
        },
        {
            "groupId": "autodiscover-srv-setup",
            "protocol": "_tcp",
            "name": "@",
            "weight": 0,
            "target": "autodiscover.mail.hostinger.com",
            "priority": 0,
            "service": "_autodiscover",
            "type": "SRV",
            "port": 443,
            "ttl": 3600
        }
    ]
}